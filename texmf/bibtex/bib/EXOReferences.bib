@article{albert_search_2018,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1707.08707},
  title = {Search for {{Neutrinoless Double}}-{{Beta Decay}} with the {{Upgraded EXO}}-200 {{Detector}}},
  volume = {120},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.120.072701},
  abstract = {Results from a search for neutrinoless double-beta decay \$0$\backslash$nu$\backslash$beta$\backslash$beta\$ of \$\^\{136\}\$Xe are presented using the first year of data taken with the upgraded EXO-200 detector. Relative to previous searches by EXO-200, the energy resolution of the detector has been improved to \$$\backslash$sigma/E\$=1.23\%, the electric field in the drift region has been raised by 50\%, and a system to suppress radon in the volume between the cryostat and lead shielding has been implemented. In addition, analysis techniques that improve topological discrimination between \$0$\backslash$nu$\backslash$beta$\backslash$beta\$ and background events have been developed. Incorporating these hardware and analysis improvements, the median 90\% confidence level \$0$\backslash$nu$\backslash$beta$\backslash$beta\$ half-life sensitivity after combining with the full data set acquired before the upgrade has increased 2-fold to \$3.7 $\backslash$times 10\^\{25\}\$ yr. No statistically significant evidence for \$0$\backslash$nu$\backslash$beta$\backslash$beta\$ is observed, leading to a lower limit on the \$0$\backslash$nu$\backslash$beta$\backslash$beta\$ half-life of \$1.8$\backslash$times10\^\{25\}\$ yr at the 90\% confidence level.},
  language = {en},
  number = {7},
  urldate = {2018-07-05},
